trait Identifiable {
  id;
}

trait SimpleName{
  name
}

trait CompoundName{
  firstName;
  lastName;
  fullName = {firstName + " " + lastName}
}

trait Addressable{
  address;
}

trait Phonable{
  phone;
}

trait GeoLocatable{
  latitude;
  longitude;
}

trait Emailable{
  email;
}

class PersonProperties{
  isA CompoundName, Addressable, Emailable;
}
